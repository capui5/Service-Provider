<mvc:View controllerName="com.sadara.ServiceProvider.controller.Home" displayBlock="true" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
	<Page showHeader="false" >
		<VBox class="hero-container"></VBox>
		<content>
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Service Provider" 
				press="onServiceProviderPress">
				<TileContent unit="EUR" footer="Current Quarter">
					<ImageContent src="sap-icon://it-host"/>
				</TileContent>
			</GenericTile>
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Services"  press="onServicePress">
				<TileContent unit="EUR" footer="Current Quarter">
					<ImageContent src="sap-icon://crm-service-manager"/>
				</TileContent>
			</GenericTile>
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Service Provider Details" 
				press="onServiceProviderDetailPress">
				<TileContent unit="EUR" footer="Current Quarter">
					<ImageContent src="sap-icon://official-service"/>
				</TileContent>
			</GenericTile>
		</content>
	</Page>
</mvc:View>